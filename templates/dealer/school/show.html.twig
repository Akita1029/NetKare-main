{% extends 'dealer/layout.html.twig' %}

{% block title %}School{% endblock %}

{% block content %}
    <h1>School</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ school.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ school.name }}</td>
            </tr>
            <tr>
                <th>LogoFilename</th>
                <td>{{ school.logoFilename }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('dealer_school_index') }}">back to list</a>

    <a href="{{ path('dealer_school_edit', {'id': school.id}) }}">edit</a>

    {{ include('dealer/school/_delete_form.html.twig') }}
{% endblock %}
