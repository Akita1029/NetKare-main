<div class="flex justify-between">
	<div>
		{% if prev %}
			<a href="{{ prev }}" class="btn btn-default">{% trans %}pagination.prev{% endtrans %}</a>
		{% endif %}
	</div>
	<div>
		{% if next %}
			<a href="{{ next }}" class="btn btn-default">{% trans %}pagination.next{% endtrans %}</a>
		{% endif %}
	</div>
</div>
