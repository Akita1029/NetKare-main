<form method="post" action="{{ path('app_admin_yearbook_delete', {'id': yearbook.id}) }}" onsubmit="return confirm('{% trans %}buttons.delete.confirm{% endtrans %}');" class="inline-block">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ yearbook.id) }}">
	<button class="btn btn-danger">{% trans %}buttons.delete.label{% endtrans %}</button>
</form>
